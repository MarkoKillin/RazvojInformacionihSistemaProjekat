package model;

import java.io.Serializable;
import jakarta.persistence.*;
import java.util.List;


/**
 * The persistent class for the uloga database table.
 * 
 */
@Entity
@NamedQuery(name="Uloga.findAll", query="SELECT u FROM Uloga u")
public class Uloga implements Serializable {
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	private int idUloga;

	private String naziv;

	//bi-directional many-to-one association to Korisnik
	@OneToMany(mappedBy="uloga")
	private List<Korisnik> korisniks;

	//bi-directional many-to-one association to Predmetstudent
	@OneToMany(mappedBy="uloga")
	private List<Predmetstudent> predmetstudents;

	//bi-directional many-to-many association to Predmet
	@ManyToMany
	@JoinTable(
		name="profesorpredmet"
		, joinColumns={
			@JoinColumn(name="Uloga_idUloga")
			}
		, inverseJoinColumns={
			@JoinColumn(name="Predmet_idPredmet")
			}
		)
	private List<Predmet> predmets;

	public Uloga() {
	}

	public int getIdUloga() {
		return this.idUloga;
	}

	public void setIdUloga(int idUloga) {
		this.idUloga = idUloga;
	}

	public String getNaziv() {
		return this.naziv;
	}

	public void setNaziv(String naziv) {
		this.naziv = naziv;
	}

	public List<Korisnik> getKorisniks() {
		return this.korisniks;
	}

	public void setKorisniks(List<Korisnik> korisniks) {
		this.korisniks = korisniks;
	}

	public Korisnik addKorisnik(Korisnik korisnik) {
		getKorisniks().add(korisnik);
		korisnik.setUloga(this);

		return korisnik;
	}

	public Korisnik removeKorisnik(Korisnik korisnik) {
		getKorisniks().remove(korisnik);
		korisnik.setUloga(null);

		return korisnik;
	}

	public List<Predmetstudent> getPredmetstudents() {
		return this.predmetstudents;
	}

	public void setPredmetstudents(List<Predmetstudent> predmetstudents) {
		this.predmetstudents = predmetstudents;
	}

	public Predmetstudent addPredmetstudent(Predmetstudent predmetstudent) {
		getPredmetstudents().add(predmetstudent);
		predmetstudent.setUloga(this);

		return predmetstudent;
	}

	public Predmetstudent removePredmetstudent(Predmetstudent predmetstudent) {
		getPredmetstudents().remove(predmetstudent);
		predmetstudent.setUloga(null);

		return predmetstudent;
	}

	public List<Predmet> getPredmets() {
		return this.predmets;
	}

	public void setPredmets(List<Predmet> predmets) {
		this.predmets = predmets;
	}

}